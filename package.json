{
  "name": "set-order",
  "version": "0.2.0",
  "description": "Tiny module for sorting by a set order, using a custom sort function for omitting explicits.",
  "main": "dist/exact-order.js",
  "scripts": {
    "build": "taskfile build",
    "test": "taskfile test",
    "lint": "taskfile lint",
    "spec": "taskfile spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wildhoney/ExactOrder.git"
  },
  "keywords": [
    "ordering",
    "order",
    "exact order",
    "explicit order",
    "specific order",
    "set order"
  ],
  "author": "Adam Timberlake <adam.timberlake@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wildhoney/ExactOrder/issues"
  },
  "homepage": "https://github.com/Wildhoney/SetOrder#readme",
  "dependencies": {
    "es6-symbol": "~3.1.1",
    "object-path": "~0.11.4",
    "ramda": "~0.23.0",
    "sort-by": "~1.2.0"
  },
  "devDependencies": {
    "ava": "~0.18.2",
    "babel-eslint": "~7.2.1",
    "babel-plugin-transform-do-expressions": "~6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "~6.24.0",
    "babel-register": "~6.24.0",
    "coveralls": "~2.13.0",
    "nyc": "~10.2.0",
    "rollup": "~0.41.6",
    "rollup-plugin-commonjs": "~8.0.2",
    "rollup-plugin-node-resolve": "~2.1.1",
    "rollup-plugin-uglify": "~1.0.1",
    "taskfile": "~0.5.3",
    "uglify-js-harmony": "~2.7.5",
    "xo": "~0.18.1"
  },
  "ava": {
    "files": [
      "tests/*.test.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "xo": {
    "space": 4,
    "parser": "babel-eslint",
    "esnext": true,
    "rules": {
      "curly": "off",
      "key-spacing": "off",
      "comma-dangle": "off",
      "padded-blocks": "off",
      "no-unused-vars": "off",
      "no-multi-spaces": "off",
      "no-implicit-coercion": "off",
      "object-curly-spacing": "off"
    }
  }
}
